<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.goods.mapper.TnGoodsSpecMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  tn_goods_spec 
		WHERE
			 goods_id = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.goods.entity.TnGoodsSpec">
		SELECT * 
		FROM  tn_goods_spec
		WHERE
			 goods_id = #{mainId} 
	</select>
</mapper>
